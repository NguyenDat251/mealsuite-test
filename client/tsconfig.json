    // "paths": { "*": ["*"], "@acme/shared-models/*": ["@acme/shared-models/*"] }
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
